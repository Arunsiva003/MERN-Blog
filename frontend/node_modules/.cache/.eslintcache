leOpen(!profileOpen)}>\r\n              <img src={PublicFlo + user.profilePic} alt='' />\r\n            </button>\r\n            {profileOpen && (\r\n              <div className='openProfile boxItems' onClick={close}>\r\n                <Link to={\"/account\"}>\r\n                  <div className='image'>\r\n                    <div className='img'>\r\n                      <img src={PublicFlo + user.profilePic} alt='' />\r\n                    </div>\r\n                    <div className='text'>\r\n                      <h4>{user.username}</h4>\r\n                      <label>Blogger</label>\r\n                    </div>\r\n                  </div>\r\n                </Link>\r\n                <Link to='/create'>\r\n                  <button className='box'>\r\n                    <RiImageAddLine className='icon' />\r\n                    <h4>Create Post</h4>\r\n                  </button>\r\n                </Link>\r\n                <Link to='/account'>\r\n                <button className='box'>\r\n                  <IoSettingsOutline className='icon' />\r\n                  <h4>My Account</h4>\r\n                </button>\r\n                </Link>\r\n                <Link to='/about'>\r\n                  <button className='box'>\r\n                  <AiOutlineHeart className='icon' />\r\n                    <h4>About</h4>\r\n                  </button>\r\n                </Link>\r\n                <Link to=\"/help\">\r\n                <button className='box'>\r\n                  <GrHelp className='icon' />\r\n                  <h4>Help</h4>\r\n                </button>\r\n                </Link>\r\n                <button className='box' onClick={handleLogout}>\r\n                  <BiLogOut className='icon' />\r\n                  {user && <h4>Log Out</h4>}\r\n                </button>\r\n              </div>\r\n            )}\r\n          </>\r\n        ) : (\r\n          <Link to='/login'>\r\n            <button>My Account</button>\r\n          </Link>\